C {devices/code_shown.sym} -20 -1355 0 0 {name=NGSPICE only_toplevel=true 
value="
.param temp=27
.param m1_w=10u
.param m2_w=10u
.param dut_w=1.0u
.param dut_l=0.13u
.param m1_l=0.13u
.param m2_l=0.13u
.option rseries 0.01
.control
save all 
dc Vgs 0 1.2 1m
let Ids = -I(E1)
let gm = deriv(Ids)
meas DC gmmax PP gm from=0 to=1.2
meas dc vmax when gm=gmmax 
meas dc Imax  find Ids when v(g2)=vmax
let b=Imax-gmmax*vmax
let vt= (1.0e-10-b)/gmmax
print vt
write tran_lshift.raw
.endc
"}
